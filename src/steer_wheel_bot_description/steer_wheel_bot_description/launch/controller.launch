<launch>

<rosparam file="$(find steer_wheel_bot_description)/launch/controller.yaml" command="load"/>
<node args="rear_wheel_position_controller front_steer_position_controller front_wheel_position_controller joint_state_controller " name="controller_spawner" ns="steer_wheel_bot" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/steer_wheel_bot/joint_states"/>
</node>

</launch>