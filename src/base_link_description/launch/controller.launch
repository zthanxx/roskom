<node args="Rev25_position_controller Rev26_position_controller Rev46_position_controller Rev47_position_controller Rev49_position_controller Rev50_position_controller joint_state_controller " name="controller_spawner" ns="base_link" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
