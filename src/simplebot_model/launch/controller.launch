<launch>

<rosparam file="$(find simplebot_model)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="simplebot" args="rw_joint_position_controller lw_joint_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/simplebot/joint_states"/>
</node>

</launch>